  <Code>'***********************************************
' Calculo do valor menos o Desconto
'***********************************************
Public Function CalcularValorComDesconto(Valor as Decimal, Desconto as Decimal) as Decimal
	
	' Retorno o valor com o desconto
	Return Valor - Valor * Desconto

End Function

'***********************************************
' Calculo do valor menos o Desconto
'***********************************************
Public Function CalcularValorComDesconto(Valor as Decimal, Desconto1 as Decimal, Desconto2 as Decimal) as Decimal
	
	' Retorno o valor com o desconto
	Return  (Valor - Valor * Desconto1) - (Valor - Valor * Desconto1) * Desconto2

End Function

'***********************************************
' Escrever texto em maisculo e minusculo
'***********************************************
Public Function RetornarTextoFormatado(Texto as String) as String
	
	Dim strTexto() as String = Texto.split(" ")
	Dim strRetorno as String = Iif(strTexto(0).Length &lt;= 2, LCase(strTexto(0)), UCase(Left(strTexto(0), 1)) &amp; Lcase(strTexto(0).SubString(1, strTexto(0).Length - 1)))

	For indice as Integer = 1 To strTexto.Length - 1
		strRetorno &amp;= " " &amp; Iif(strTexto(indice).Length &lt;= 2, LCase(strTexto(indice)), UCase(Left(strTexto(indice), 1)) &amp; Lcase(strTexto(indice).SubString(1, strTexto(indice).Length - 1)))
	Next
	
	' Retorno o valor formatado
	Return  strRetorno

End Function</Code>
